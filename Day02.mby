b+TkAL9pzXTil++6nzfj847hQzuCkwD0tISgfvcjeY6dGm/\n+EhR8DTApU17mM10T6z6dMWHGTngqzTTWmJi7zN2pMCD0dVS+/vVm7sJqcQm\nTZi9jY20YARci74rLycNU25zMw0mQLg78TkJ9J2woImxztO8wuwnz6Yqy8Oj\n+taCSs6O1uxs7PsDwwZ5O0nDhrhiYpNgUhn87cQwKUbvEDQKQSNoBI0IEDSC\nRiFoBI2gUQgaQaMQNIJG0CgEjaBRCBpBI2gUgkbQKASNoBE0CkEjaBSCRtAI\nGoWgETQKQSNoBI1C0N+I/gNIVS9XijG/SAAAAABJRU5ErkJggg==\n"}}},"main.py":"import rtc\nimport switch\nfrom time import sleep\nimport display\nimport iKB1\n\ndef __limit__ (val,minVal,maxVal) :\n    return max(minVal,(min(val,maxVal)))\n\nclass Sensors5 :\n    def __init__ (self, threshold = [600,600,600,600,600]):\n        self.value = [None,None,None,None,None]\n        self.threshold = threshold.copy()\n\n    def update(self):\n        self.value[0] = iKB1.ADC(0).read()\n        self.value[1] = iKB1.ADC(1).read()\n        self.value[2] = iKB1.ADC(2).read()\n        self.value[3] = iKB1.ADC(3).read()\n        self.value[4] = iKB1.ADC(4).read()\n\n    def getSensors (self,autoUpdate = True):\n        if autoUpdate : self.update()\n        return [self.value]\n\n    def isBlack(self,autoUpdate = True):\n        if autoUpdate : self.update()\n        return [self.value[0] > self.threshold[0] ,self.value[1] > self.threshold[1], self.value[2] > self.threshold[2], self.value[3] > self.threshold[3], self.value[4] > self.threshold[4]]\n\nclass GripperController :\n    def __init__(self,pullupPin = 1,grapPin = 2):\n        self.pullupServo = iKB1.Servo(pullupPin)\n        self.grapServo = iKB1.Servo(grapPin)\n    def __limit__ (val,minVal,maxVal) :\n        return max(minVal,(min(val,maxVal)))\n\n    def grap (self):\n        self.grapServo.angle(90)\n\n    def ungrap (self):\n        self.grapServo.angle(30)\n\n    def pullup (self):\n        self.pullupServo.angle(90)\n    def pulldown (self):\n        self.pullupServo.angle(0)\n\nclass MotorController :\n    def __init__ (self,leftCh = 1,rightCh = 2):\n        self.leftMotor = iKB1.Motor(1)\n        self.rightMotor = iKB1.Motor(2)\n    def __limit__ (self,val,minVal,maxVal) :\n        return max(minVal,(min(val,maxVal)))\n\n    def __setDirAndSpd__ (self,motor,spd):\n        if spd >= 0 :\n            motor.dir(iKB1.Motor.FORWARD)\n            motor.speed(spd)\n        else :\n            motor.dir(iKB1.Motor.BACKWARD)\n            motor.speed(spd * -1)\n\n    def leftWheel (self,spd):\n        self.__setDirAndSpd__(self.leftMotor,self.__limit__(spd,0,100))\n        leftMotor.update()\n    def rightWheel (self,spd):\n        self.__setDirAndSpd__(self.rightMotor,self.__limit__(spd,0,100))\n        rightMotor.update()\n    def motors (self,lSpd,rSpd):\n        self.__setDirAndSpd__(self.leftMotor,self.__limit__(lSpd,0,100))\n        self.__setDirAndSpd__(self.rightMotor,self.__limit__(rSpd,0,100))\n        self.leftMotor.update()\n        self.rightMotor.update()\n\nclass MovementController :\n    def __init__ (self,gripper,sensors,motor) :\n        self.Gripper = gripper\n        self.Sensors = sensors\n        self.Motor = motor\n        self.trackErrorValue = 0\n    def __limit__ (val,minVal,maxVal) :\n        return max(minVal,(min(val,maxVal)))\n\n    def __getErrorValue__ (self):\n        pass\n\n\n\ndef main ():\n    sensors = Sensors5(threshold = [600,600,600,600,600])\n    gripper = GripperController(1,2)\n    motors = MotorController(1,2)\n    controller = MovementController(gripper,sensors,motors)\n\n    while True : #mainloop()\n        pass\n\nmain()\n"}